{
	"name": "frontend",
	"private": true,
	"author": "Divya Pankaja Nanda",
	"license": "MIT",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"clean": "rimraf dist",
		"prebuild": "npm run clean",
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "eslint .",
		"preview": "vite preview",
		"format:fix": "npx prettier --write 'src/**/*.ts' 'src/**/*.tsx'",
		"format:check": "npx prettier --check 'src/**/*.ts' 'src/**/*.tsx'",
		"lint:fix": "npx eslint 'src/**/*.ts' 'src/**/*.tsx' --fix",
		"lint:check": "npx eslint 'src/**/*.ts' 'src/**/*.tsx'",
		"sign": "node scripts/addHeader.js $(find src -type f \\( -name \"*.ts\" -o -name \"*.tsx\" \\))",
		"prepare": "[ \"$VITE_PRODUCTION_ENVIRONMENT\" = \"true\" ]; || husky install"
	},
	"lint-staged": {
		"src/**/*.{ts.tsx}": [
			"node scripts/addHeader.js",
			"npm run lint:check",
			"npm run format:check"
		]
	},
	"dependencies": {
		"axios": "^1.8.4",
		"jsonwebtoken": "^9.0.2",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-router": "^7.3.0",
		"zod": "^3.24.2",
		"zustand": "^5.0.3"
	},
	"devDependencies": {
		"@eslint/js": "^9.21.0",
		"@tailwindcss/vite": "^4.0.14",
		"@types/jsonwebtoken": "^9.0.9",
		"@types/react": "^19.0.10",
		"@types/react-dom": "^19.0.4",
		"@vitejs/plugin-react": "^4.3.4",
		"eslint": "^9.21.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-react-hooks": "^5.1.0",
		"eslint-plugin-react-refresh": "^0.4.19",
		"globals": "^15.15.0",
		"husky": "^8.0.3",
		"lint-staged": "^15.5.0",
		"npm-run-all": "^4.1.5",
		"prettier": "3.5.3",
		"rimraf": "^6.0.1",
		"tailwindcss": "^4.0.14",
		"typescript": "~5.7.2",
		"typescript-eslint": "^8.24.1",
		"vite": "^6.2.0"
	}
}
